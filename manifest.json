{
  "name": "Piped Redirector",
  "version": "0.1",
  "description": "Redirects Youtube links to Piped",
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules.json"
    }]
  },
  "permissions": [
    "*://*.youtube.com/*",
    "declarativeNetRequest"
  ],
  "manifest_version": 2,
  "icons": {
    "16": "/images/16x16.png",
    "32": "/images/32x32.png",
    "48": "/images/48x48.png",
    "128": "/images/128x128.png"
  }
}